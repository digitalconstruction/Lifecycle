        <rdfs:label>consumesFrom</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/Lifecycle#hasLODLevel -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasLODLevel">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/Variables#isApplicableIn"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#LODLevel"/>
        <terms:description>Describes the property values and their level of detail regarding to the specific LOD Framework</terms:description>
        <rdfs:comment>Relationship between property state and level of detail.</rdfs:comment>
        <rdfs:label>hasLODLevel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/Lifecycle#hasLevel -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasLevel">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/Contexts#hasContext"/>
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#isLevelOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#LOD_Framework"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#LODLevel"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasLevel"/>
            <rdf:Description rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasSubLevel"/>
        </owl:propertyChainAxiom>
        <terms:description>Enables the connection betweendifferent levels to its framework</terms:description>
        <rdfs:label>hasLevel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/Lifecycle#hasNextLevel -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasNextLevel">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/Contexts#nextContext"/>
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#hasPreviousLevel"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#LODLevel"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#LODLevel"/>
        <terms:description>Indicates the sequence of levels.</terms:description>
        <rdfs:label>hasNextLevel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/Lifecycle#hasNextStage -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasNextStage">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/Contexts#nextContext"/>
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#hasPreviousStage"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#BuildingLifecycleStage"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#BuildingLifecycleStage"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasNextStage"/>
            <rdf:Description rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasSubStage"/>
        </owl:propertyChainAxiom>
        <terms:description>Enables the relation between the stages and defines the sequence of the stages.</terms:description>
        <rdfs:label>hasNextStage</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/Lifecycle#hasPreviousLevel -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasPreviousLevel">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/Contexts#previousContext"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#LODLevel"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#LODLevel"/>
        <terms:description>Indicates the sequence of levels.</terms:description>
        <rdfs:label>hasPreviousLevel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/Lifecycle#hasPreviousStage -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasPreviousStage">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/Contexts#previousContext"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#BuildingLifecycleStage"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#BuildingLifecycleStage"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasPreviousStage"/>
            <rdf:Description rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasSubStage"/>
        </owl:propertyChainAxiom>
        <terms:description>Enables the relation between the stages and defines the sequence of the stages.</terms:description>
        <rdfs:label>hasPreviousStage</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/Lifecycle#hasStage -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasStage">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/Contexts#hasContext"/>
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#isStageOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#BLCS_Framework"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#BuildingLifecycleStage"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasStage"/>
            <rdf:Description rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasSubStage"/>
        </owl:propertyChainAxiom>
        <terms:description>Enables the relationship between the BLCS framework and different stages involved in it.</terms:description>
        <rdfs:label>hasStage</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/Lifecycle#hasSubLevel -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasSubLevel">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/Contexts#hasSubContext"/>
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#hasSuperLevel"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#LODLevel"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#LODLevel"/>
        <terms:description>Represents the relation between Upper levels and its sub level</terms:description>
        <rdfs:label>hasSubLevel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/Lifecycle#hasSubStage -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasSubStage">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/Contexts#hasSubContext"/>
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#hasSuperStage"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#BuildingLifecycleStage"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/Lifecycle#BuildingLifecycleStage"/>
        <terms:description>Describe the relationship between main and its sub stages involved in a specific BLCS framework.</terms:description>
        <rdfs:label>hasSubStage</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/Lifecycle#hasSuperLevel -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasSuperLevel">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/Contexts#isSubContextOf"/>
