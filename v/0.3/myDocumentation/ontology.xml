    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/Lifecycle#consumesFrom -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/Lifecycle#consumesFrom">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/Entities#isAgentIn"/>
