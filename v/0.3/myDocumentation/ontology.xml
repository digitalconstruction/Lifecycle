    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/Lifecycle#isLevelOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/Contexts#isContextOf"/>
