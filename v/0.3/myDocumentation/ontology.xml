        <rdfs:comment>Represents the relation between Upper levels and its sub level</rdfs:comment>
        <rdfs:label>hasSuperLevel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/Lifecycle#hasSuperStage -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/Lifecycle#hasSuperStage">
