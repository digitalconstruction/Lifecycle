        <rdfs:label>requires</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/Lifecycle#wasNeededFor -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/Lifecycle#wasNeededFor">
