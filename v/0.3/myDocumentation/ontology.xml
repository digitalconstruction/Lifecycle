        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/Entities#Activity"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/Entities#isRoleOf -->


    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/Entities#isRoleOf">
